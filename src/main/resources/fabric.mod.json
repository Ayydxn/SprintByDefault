{
  "schemaVersion": 1,
  "id": "sprint-by-default",
  "version": "${version}",
  "name": "Sprint By Default",
  "description": "A simple mod which adds the ability for the player to always sprint when moving forwards.",
  "authors": [
    "YoArcane"
  ],
  "contact": {
    "homepage": "https://github.com/YoArcane/SprintByDefault",
    "sources": "https://github.com/YoArcane/SprintByDefault",
    "issues": "https://github.com/YoArcane/SprintByDefault/issues"
  },
  "license": "LGPL-3.0",
  "icon": "assets/sprint-by-default/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.yoarcane.sprintbydefault.SprintByDefaultClientMod"
    ]
  },
  "mixins": [
    "sprint-by-default.mixins.json"
  ],
  "depends": {
    "minecraft": ">=1.20",
    "fabricloader": ">=${loader_version}",
    "fabric": "*"
  }
}
